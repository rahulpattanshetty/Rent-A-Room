<h3>Booking History</h3>
<table border="1">
	<thead>
		<tr>
			<th>Room</th>
			<th>Start Date</th>
			<th>End Date</th>
			<th>Price</th>
		</tr>
	</thead>
	<tbody>
		<%@bookings.each do |booking|%>
			<tr>
				<th><%=booking.room.name%></th>
				<th><%=booking.start_date%></th>
				<th><%=booking.end_date%></th>
				<th><%=booking.price%></th>
			</tr>
			<%end%>
	</tbody>
</table>